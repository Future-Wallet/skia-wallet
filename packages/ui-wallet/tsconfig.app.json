{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "../../dist/out-tsc",
    "types": [
      "node"
    ],
  },
  "paths": {
    "stream": [
      "./node_modules/readable-stream"
    ]
  },
  "files": [
    "../../node_modules/@nrwl/react/typings/cssmodule.d.ts",
    "../../node_modules/@nrwl/react/typings/image.d.ts"
  ],
  "exclude": [
    "./src/utils/contracts/**",
    "./src/utils/mnemonicComponent.ts",
    "jest.config.ts",
    "**/*.spec.ts",
    "**/*.test.ts",
    "**/*.spec.tsx",
    "**/*.test.tsx",
  ],
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ]
}